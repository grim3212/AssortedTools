{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "issues": "${issue_tracker}"
  },
  "license": "${license}",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.grim3212.assorted.tools.AssortedToolsFabric"
    ],
    "client": [
      "com.grim3212.assorted.tools.client.ToolsClientFabric"
    ],
    "fabric-datagen": [
      "com.grim3212.assorted.tools.common.data.AssortedToolsFabricDatagen"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric-api": "*",
    "minecraft": "${fabric_mc_version_range}",
    "java": ">=17",
    "assortedlib": ">=1.0.1-25"
  }
}
